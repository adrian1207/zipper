{
    "name": "adrian1207/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/riteshsingh1/zipper",
    "license": "MIT",
    "authors": [
        {
            "name": "Ritesh Singh",
            "email": "hello@imritesh.com",
            "homepage": "https://imritesh.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-zip": "*",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "imritesh\\Zipper\\": "src/imritesh/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "imritesh\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "imritesh\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "imritesh\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
